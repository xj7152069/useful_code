alias xj_tim='/home/dell/wine_soft/TIM-x86_64.AppImage'
alias xj_701_o='ssh -l xj -p 701 xj@27.115.40.122'
alias xj_701_i='ssh xj@192.168.1.200'
alias xj_mathtype='wine /home/dell/wine_soft/MathType/MathType.exe'

ximage_shell="ximage<filename n1=101 wbox=1500 hbox=500 legend=1 f1=0 d1=10 f2=0 d2=10 label1='(m)' label2='(m)'"
alias xj_ximage='echo $ximage_shell'
scp701="scp -P 701 -r 文件夹名(被复制对象) [-f 文件名] xj@27.115.40.122:集群文件路径（目标路径）"
alias xj_701_scp='echo $scp701'
qq_mail_key="POP3:zpnnvcqpbuqzgffi ; IMAP:rqybnvfhgvcehdif"
alias xj_qq_mail_secret='echo $qq_mail_key'
xmovie_shell="xmovie<movie.bin n1=301 n2=301 width=1000 height=1000 sleep= perc= bclip=0.01 wclip=-0.01"
alias xj_xmovie='echo $xmovie_shell'


# added by Anaconda3 installer
export PATH="/home/dell/anaconda3/bin:$PATH"



